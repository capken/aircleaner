<script type="text/javascript">
//<![CDATA[
// create an array object to hold page specific variables needed in ajax requests
var aParamsToPass = {
productId: "AC4030_00_CN_CONSUMER",
catalogType: "consumer",
country: "cn",
language: "zh",
privacyUrl: "",
modelNumber: "AC4030/00",
retailStoreStatus: "",
onlineStoreStatus: "",
productCTN: "AC4030/00",
productCategoryID: "AIR_PURIFIER_CA_CN_CONSUMER"
}
function returnTabName(s) {
if( s.indexOf(" ") != -1 ) {
s = s.substring(0, s.indexOf(" "));
}
return "." +  s + "_content";
}
function submitForm(theform) {
if( theform.name == 'newsLetterForm' || theform.name == 'confirmEmailForm' ) {
aParamsToPass['email'] = theform.email.value;
vSuccessString = "<div class='iconimg'><img src='/consumerfiles/assets/img/SubscribeEmail-IconBig.png' /></div><div class='congrats'>您将收到飞利浦的每月简讯。</div>";
}
if( theform.name == 'newsLetterForm' ) { vURL = "/c/common/fragments/newsletterform.jsp" }
if( theform.name == 'confirmEmailForm' ) { vURL = "/c/common/fragments/confirmemailform.jsp" }
if( theform.name == 'wulEmailForm' ) {
aParamsToPass['email'] = theform.email.value;
aParamsToPass['newsLetterType'] = 'WUL';
vURL = "/c/common/fragments/confirmemailform.jsp";
vSuccessString = "<div class='retmsg'>谢谢！ 您的信息已发送</div>";
}
if( theform.name == 'tellAFriendForm' ) {
aParamsToPass['friendsname'] = theform.friendsname.value;
aParamsToPass['friendsemail'] = theform.friendsemail.value;
aParamsToPass['yourmessage'] = theform.yourmessage.value;
aParamsToPass['yourfirstname'] = theform.yourfirstname.value;
aParamsToPass['yourlastname'] = theform.yourlastname.value;
aParamsToPass['youremail'] = theform.youremail.value;
vURL = "/c/common/fragments/tellafriendform.jsp";
vSuccessString = "<div class='congrats'>谢谢！ 您的信息已发送</div>";
}
// Form submit logic for newsletter subscription
$.ajax({
type: "GET",
dataType: "json",
data: aParamsToPass,
url: vURL,
success: function(resp){
var Response = eval(resp);
if(Response.Submission_status == "Failure") {
$(theform.elements).siblings("span").removeClass("error");
$("div.errormessage").remove();
if(Response.Errors[0].error == 'System error') {
$("<div class='errormessage error'>抱歉，此服务当前不可用。</div>").prependTo(theform);
} else {
$("<div class='errormessage error'>" + Response.Errors[0].error + "</div>").prependTo(theform);
for(i=0;i<Response.Errors.length;i++) {
$(".pce_options .option_newsletter .message").empty();
$("." + Response.Errors[i].field).addClass("error");
}
}
} else {
if( theform.name == 'newsLetterForm' ) {
$("div.option_newsletter").replaceWith(vSuccessString);
} else if( theform.name == 'wulEmailForm' ) {
$("div.errormessage").remove();
$("div.option_wulnewsletter").replaceWith(vSuccessString);
} else {
$(".popuplayer .content").replaceWith(vSuccessString);
if( theform.name == 'confirmEmailForm' ) {
setOmnitureLinksWithEvents('newsletter','event11','true');
$("#confirmEmailForm").empty();
$("#confirmEmailForm").append(vSuccessString);
}
window.setTimeout("pceHideMe('.popuplayer')", 5000);
}
}
},
error: function(resp) {
if( theform.name == 'newsLetterForm' ) {
$("div.option_newsletter").replaceWith(resp);
} else if( theform.name == 'wulEmailForm' ) {
$("div.option_wulnewsletter").replaceWith(resp);
} else {
$(".popuplayer .content").replaceWith(resp);
}
}
});
}
$(document).ready(function() {
// Tell a friend layer
$("span.tellAFriend").click( function() {loadLayer( this, "/c/common/fragments/pLayer_tellAFriend.jsp", aParamsToPass )} );
// tellAFriendSubmit. Uses jquery 'live' binding that works with ajax loaded content
$(".tellAFriendSubmit").live("click", function(){
submitForm( document.tellAFriendForm );
});
// news letter registration.
$(".newsLetterRegistration").click( function() {submitForm( document.confirmEmailForm )});
// WUL email registration.
$(".wulEmailRegistration").click( function() {submitForm( document.wulEmailForm )});
// Awards popup layer
$(".awardMore").click( function(event) {
var thisClass = this.className;
var awardNum = thisClass.substring( thisClass.indexOf('_')+1 );
if (typeof window.innerWidth != 'undefined') {
viewportwidth = window.innerWidth;
}
// IE6 in standards compliant mode (i.e. with a valid doctype as the first line in the document)
else if (typeof document.documentElement !='undefined'
&& typeof document.documentElement.clientWidth !='undefined'
&& document.documentElement.clientWidth != 0) {
viewportwidth = document.documentElement.clientWidth;
}
var popupWidth = 500;
var leftPos = event.pageX-30;
var topPos = event.pageY-20;
if(leftPos + popupWidth >= viewportwidth) {
leftPos -= 420;
}
$.each('.awardsPopup', function(i) {
i = i+1;
if( i == parseInt(awardNum) && $('.awardsPopup.award_' + i).css('display') == 'block') {
return false;
} else {
$('.awardsPopup.award_' + i ).hide()
}
});
$('.awardsPopup.award_' + awardNum).css({"top":topPos, "left":leftPos});
$('.awardsPopup.award_' + awardNum).show();
});
// Logo popup layer
$(".logoMore").click( function(event) {
var thisClass = this.className;
var awardNum = thisClass.substring( thisClass.indexOf('_')+1 );
if (typeof window.innerWidth != 'undefined') {
viewportwidth = window.innerWidth;
}
// IE6 in standards compliant mode (i.e. with a valid doctype as the first line in the document)
else if (typeof document.documentElement !='undefined'
&& typeof document.documentElement.clientWidth !='undefined'
&& document.documentElement.clientWidth != 0) {
viewportwidth = document.documentElement.clientWidth;
}
var popupWidth = 500;
var leftPos = event.pageX-30;
var topPos = event.pageY-20;
if(leftPos + popupWidth >= viewportwidth) {
leftPos -= 420;
}
$.each('.logoPopup', function(i) {
i = i+1;
if( i == parseInt(awardNum) && $('.logoPopup.logo_' + i).css('display') == 'block') {
return false;
} else {
$('.logoPopup.logo_' + i ).hide()
}
});
$('.logoPopup.logo_' + awardNum).css({"top":topPos, "left":leftPos});
$('.logoPopup.logo_' + awardNum).show();
});
$.ajax({
type: 'GET',
url: "http://www.philips.com.cn/e/bin/promotionchina/imagelist?ProductId="+ aParamsToPass.productCTN + "&CategoryId=" + aParamsToPass.productCategoryID+ "&pdp=true&callback=PromotionInfo",
async: false,
jsonpCallback: 'PromotionInfo',
contentType: "application/json",
dataType: 'jsonp',
success: function(json) {
if(json.promoInfo.bannerType == "URL") {
$("#pdp_bannerblock").append("<div  class=\"block1\"><div class=\"block1a\"> <img width=\"71\" height=\"17\"  src=\"/consumerfiles/pageitems/locales/zh_CN/CONSUMER/categorypages/OneChinaDetailPageBanner/heroPrdt_title2.png\"></div> <div class=\"block1a\" id=\"pdp_promotion_banner\"><a href=\""+ json.promoInfo.promotionDetailPage +"\"> <img width=\"235\" height=\"105\" src=\""+json.promoInfo.promoBanner+"\"></a></div></div>");
}else{
var HTMLResponse = json.promoInfo.promoBanner;
HTMLResponse = HTMLResponse.replace("\/","/");
$("#pdp_bannerblock").append("<div  class=\"block1\"><div class=\"block1a\"> <img width=\"71\" height=\"17\"  src=\"/consumerfiles/pageitems/locales/zh_CN/CONSUMER/categorypages/OneChinaDetailPageBanner/heroPrdt_title2.png\"></div> <div class=\"block1a\" id=\"pdp_promotion_banner\"><a href=\""+ json.promoInfo.promotionDetailPage +"\"> "+  HTMLResponse +"</a></div></div>");
}
$("#pdp_promotion_banner").bind("click",function(event){
event.preventDefault();
event.stopPropagation();
_page.metrics.trackNavContent ({navdest:"promotion_center", navid:"pdp_banner"});
window.open($(this).find("a")[0].href, '_blank');
});
},
error: function(e) {
// console.log(e.message);
}
});
/*
// Initialize the like counter logic
/*Philips.Social.Like.Product({
"language": "zh",
"country": "cn",
