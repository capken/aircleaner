<script type="text/javascript">
var currentLocale = "cn";
if(currentLocale  !== 'cn'){
$('.cat-scroller-item-image').bind('click',function(event){
event.preventDefault();
var checkLocaleExist, getLocale, locale, checkLocale, checkLocaleStr,  checkLocaleStr, strUptoLocale,  getArr,  slicedArr, clickedClass,  requiredStr;
var  clickedCategory = $(this).parent().parent().attr('data-cat');
checkLocaleExist = clickedCategory.indexOf(':');
getLocale = clickedCategory.substr(checkLocaleExist + 1,2)
locale = getLocale.toUpperCase();
checkLocale = clickedCategory.indexOf(':'+locale+'P');
checkLocaleStr = clickedCategory.indexOf('_'+locale+'_');
strUptoLocale = clickedCategory.substr(0,checkLocaleStr);
getArr = strUptoLocale.split('_');
//console.log('checkLocale: ' + checkLocale);
if(checkLocale >= 0){
slicedArr = getArr.slice(0,getArr.length);
}else{
slicedArr = getArr.slice(0,getArr.length - 1);
}
requiredStr = slicedArr.join('_');
//console.log('requiredStr: ' + requiredStr);
_page.metrics.trackNavIpn ({ navdest:requiredStr, navid:'img' });
window.location.href = $(this).parent().attr('href');
});
$('.cat-scroller-item-desc').bind('click',function(event){
event.preventDefault();
var checkLocaleExist, getLocale, locale, checkLocale, checkLocaleStr,  checkLocaleStr, strUptoLocale,  getArr,  slicedArr, clickedClass,  requiredStr;
var  clickedCategory = $(this).parent().parent().attr('data-cat');
checkLocaleExist = clickedCategory.indexOf(':');
getLocale = clickedCategory.substr(checkLocaleExist + 1,2)
locale = getLocale.toUpperCase();
checkLocale = clickedCategory.indexOf(':'+locale+'P');
checkLocaleStr = clickedCategory.indexOf('_'+locale+'_');
strUptoLocale = clickedCategory.substr(0,checkLocaleStr);
getArr = strUptoLocale.split('_');
//console.log('checkLocale: ' + checkLocale);
if(checkLocale >= 0){
slicedArr = getArr.slice(0,getArr.length);
}else{
slicedArr = getArr.slice(0,getArr.length - 1);
}
requiredStr = slicedArr.join('_');
//console.log('requiredStr: ' + requiredStr);
_page.metrics.trackNavIpn ({ navdest:requiredStr, navid:'txt' });
window.location.href = $(this).parent().attr('href');
});
}
$('#ipn-navigation .inner-more').bind('click',function(event){
event.preventDefault();
var checkLocaleExist, getLocale, locale, checkLocale, checkLocaleStr,  checkLocaleStr, strUptoLocale,  getArr,  slicedArr, clickedClass,  requiredStr;
var  clickedCategory = $(this).attr('data-cat');
checkLocaleExist = clickedCategory.indexOf(':');
getLocale = clickedCategory.substr(checkLocaleExist + 1,2)
locale = getLocale.toUpperCase();
checkLocale = clickedCategory.indexOf(':'+locale+'P');
checkLocaleStr = clickedCategory.indexOf('_'+locale+'_');
strUptoLocale = clickedCategory.substr(0,checkLocaleStr);
getArr = strUptoLocale.split('_');
//console.log('checkLocale: ' + checkLocale);
if(checkLocale >= 0){
slicedArr = getArr.slice(0,getArr.length);
}else{
slicedArr = getArr.slice(0,getArr.length - 1);
}
requiredStr = slicedArr.join('_');
//console.log('requiredStr: ' + requiredStr);
_page.metrics.trackNavIpn ({ navdest:requiredStr, navid:'txt' });
window.location.href = $(this).find("a")[0].href;
});
</script>
