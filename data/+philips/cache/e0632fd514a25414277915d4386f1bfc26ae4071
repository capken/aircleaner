<script type="text/javascript">
//<![CDATA[
function submitForm(theform) {
var validatedVal = validate(theform.email.value);
function validate(email) {
var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
if (reg.test(email) == false)
{
document.getElementById('email').value = "";
document.getElementById("email").focus();
}
else{
validatedVal = email;
}
return validatedVal;
}
$.ajax({
type: "GET",
dataType: "json",
data: "email=" +validatedVal+ "&country=cn&catalogType=consumer&language=zh",
url: "/c/common/fragments/confirmemailform.jsp",
success: function(resp){
var Response = eval(resp);
if(Response.Submission_status == "Failure") {
$(theform.elements).siblings("span").removeClass("error");
$(".pi_NewsLetterRegister_v2 div.errormessage").remove();
if(Response.Errors[0].error == 'System error') {
$("<div class='errormessage error'>抱歉，此服务当前不可用。</div>").prependTo(theform);
} else {
$(".pi_NewsLetterRegister_v2 .content").empty();
$("<div class='errormessage error'>" + Response.Errors[0].error + "</div>").prependTo(theform);
for(i=0;i<Response.Errors.length;i++) {
$("." + Response.Errors[i].field).addClass("error");
}
}
} else {
setOmnitureSubscribeButton();
$(".errormessage").remove();
$(".pi_NewsLetterRegister_v2 #confirmEmailForm").remove();
$(".pi_NewsLetterRegister_v2").append("<p class='content'>感谢您订阅！</p><p class='content'>您将收到一封有关确认订阅的电子邮件</p>");
}
},
error: function(resp) {
$(".popuplayer .content").empty();
$(".popuplayer .content").append(resp);
}
});
}
//]]>
</script>
