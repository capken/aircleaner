var mouseover_t = [];
var mouseout_t = [];
$('.t-val0').each(function(index){
$(this).hover( //鼠标经过
function(){
    var s = this;
    clearTimeout(mouseout_t[index]);  // 停止卷起事件
    // 当鼠标进入超过 0.2 秒, 展开菜单, 并记录到线程 ID 中
                mouseover_t[index] = setTimeout(function() {
                    $('.t-val1b').stop(true).animate({
                            'bottom':'57px'
                        }, 300);                    
                }, 200); 
            },
 function(){ // 鼠标离开
    var s = this;               
    clearTimeout(mouseover_t[index]); //停止展开事件
    // 当鼠标离开超过 0.5 秒, 卷起菜单, 并记录到线程 ID 中
                mouseout_t[index] = setTimeout(function() {
                    $('.t-val1b').stop(true).animate({
                            'bottom':'-44px'
                        }, 300);   /*默认 下来.t-val1b ul*/           
                }, 200);
        });
 });
});
