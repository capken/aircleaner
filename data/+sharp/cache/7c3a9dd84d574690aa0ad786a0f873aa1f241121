  $(document).ready(function(){
      $("div.back_top").click(function(){
        $('html, body').animate({scrollTop:0}, 'slow');
        return false;
        });
      $("ul.idTabs a").click(function(){
        $("ul.idTabs a").each(function(){
          var id_ele = $(this).attr("data");
          $(id_ele).hide()
          }); 
        $($(this).attr("data")).show();
        button_now($(this).attr("id"));
        $('html, body').animate({scrollTop:"650px"}, 500);
        return false;  
      }); 
  })
