	<script type="text/javascript">

	function logoutyl(){
	if($('#iframeyulan').length <=0){
	
	$("body").after("<iframe id='iframeyulan'></iframe>")
	
	$('#iframeyulan').attr('src', 'http://support-cn.samsung.com/MySamsungCN/Club/launchevent/api/logout.ashx');
	 $('#iframeyulan').load(function()
	 {
		
		 location.href = "http://club.samsung.com/cn/login/SALogout"
	 });
	
	}
	}

    function logoutzuoyou(){
    var logoutUrl = location.href.split("://")[0] + "://support-cn.samsung.com/MySamsungCN/Club/launchevent/api/logout.ashx";
    $.ajax({
    type: "POST",
    url: logoutUrl,
    async: false,
    data: "",
    success: function (data) {
    if (data == 1) {
    location.href = "http://club.samsung.com/cn/login/memberLogOut";
    } else {
    // alert(data);
    }
    }
    });

    }

    $(function(){
    var prof_id = getCookie("iPlanetDirectoryPro");
    if ((prof_id != null) && (prof_id != "")){
			$("#mymenu_logout").show();
			$("#mymenu_logout2").show();
			$("#loginId").hide();}
		else
		{
			$("#mymenu_logout").hide();
			$("#mymenu_logout2").hide();
			$("#loginId").show();
		}
	}); 
  
	  
	  function logout() {
	  deleteCookie("prof_country", "/", ".samsung.com");
	  deleteCookie("prof_id", "/", ".samsung.com");
	  deleteCookie("prof_prolist", "/", ".samsung.com");
	  deleteCookie("prof_username", "/", ".samsung.com");
	  deleteCookie("prof_pass_info", "/", ".samsung.com");
	  deleteCookie("bvdisplaycode", "/", "");
	  deleteCookie("bvproductid", "/", "");
	  deleteCookie("bvpage", "/", "");
	  deleteCookie("bvcontenttype", "/", "");
	  deleteCookie("bvauthenticateuser", "/", "");
	
	  deleteCookie("bzv_url", "/", "");
	  deleteCookie("auth_flag", "/", "");
	
	  //window.location.reload();
	  //window.location.href = document.URL;
	  window.location.href="http://support-cn.samsung.com/MySamsungCN/logout.aspx";
	  }
	  function deleteCookie(name, path, domain)
	  {
	  if (getCookie(name))
	  document.cookie = name + '=' +
	  ((path) ? ';path=' + path : '') +
	  ((domain) ? ';domain=' + domain : '' ) +
	  ';expires=Thu, 01-Jan-1970 00:00:01 GMT';
	  }
	</script>
		
		
