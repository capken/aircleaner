		function trick() {
		    if (navigator.userAgent.indexOf("MSIE") != -1) return;
		
		    var target = $('#search');
		    if ( onpressTrick != target.val() && '' != target.val() ) {
			onpressTrick = target.val();
			//sqs.keyUp( eventTrick );
			if (g_sqs != null && eventTrick != undefined && eventTrick.keyCode != 40 && eventTrick.keyCode != 38) g_sqs.fetchAndDisplaySuggestions();
		    }
		}
		    
		setInterval("trick()", 300);
		function tricks(e) { eventTrick = e; };
