<script language="javascript">
function getCookie(name)
{
	var search = name + "="
	if(document.cookie.length > 0) {
	    offset = document.cookie.indexOf(search)
	    if(offset != -1){
	      offset += search.length
	      end = document.cookie.indexOf(";", offset)
	      if(end == -1)
	        end = document.cookie.length
	    return document.cookie.substring(offset, end)
	    }
	}
    	return "";
}
function openPopup() 
{
  if(!IsPop01())
  {
   return 
   }
 var _supportUrl=document.location.href;
if(_supportUrl.indexOf("/cn/support/main/supportMain.do")>0)
{	
	var eventCookie=getCookie("index");
	if (eventCookie != "no")
	{	
		var URL ="/cn/pop/20130628/popup_post.html"; 
		var status = "toolbar=no,directories=no,scrollbars=2,resizable=no,status=no,menubar=no,width=500, height=570, top=0,left=0"; 
		if(IsPop01()){
			window.open(URL,"windowname",status)
		}
    }
 }
}
function IsPop01(){	
	var flag = false;
	var date = new Date();
	var year = date.getYear();
	var month = date.getMonth();
	var day = date.getDate();
	var hour = date.getHours();
	var flagDate = new Date(2013,05,28,08);
	var lastDate = new Date(2013,06,07,21);//时间大于2月8日的时候弹出
	//alert(year+"-"+month+"-"+day+"-"+hour);
	if(date>flagDate && date<lastDate){
		flag = true;
	}
   return flag;	
}
$(document).ready(function ()
{
 openPopup();
}
)
</script>

