	<script type="text/javascript">
		
		var share_site_cd 		= "cn";
		var share_model_name 	= "SA-T501BA";
		var share_display_name  = "SA501TBCH";
		var share_detail_url 	= "/cn/consumer/home-appliances/air-care/virus-doctor/SA-T501BA";
	
		// make a request 처리용
		function makeRequest(mkRqGbn,cntusUrl,linkType,width,height,scrollFl) {
			if(cntusUrl=='') {
				if("PT"==mkRqGbn || "ST"==mkRqGbn || "IST"==mkRqGbn || "RT"==mkRqGbn || "RD"==mkRqGbn) {
					cntusUrl = "/cn/business/bizTypeRequest.do";
					if("PT"==mkRqGbn || "ST"==mkRqGbn) {
						cntusUrl += "?iaCode=07140300";	
					}
				} else if("PD"==mkRqGbn) {
					cntusUrl = "/cn/business/bizProductRequest.do?iaCode=07140300&modelCode=SA-T501BA";
				} else if("SD"==mkRqGbn) {
					cntusUrl = "/cn/business/bizSolutionRequest.do?iaCode=07140300&modelCode=SA-T501BA";
				}
			}
			
			if("NEW"==linkType) {
				window.open(cntusUrl,"makeReqPopup");	
			} else {
				var scrollbars = "no";
				if("Y"==scrollFl) scrollbars = "yes";
				window.open(cntusUrl,"makeReqPopup","width="+width+",height="+height+",scrollbars="+scrollbars);
			}				
		}
		
					
	</script>

		

