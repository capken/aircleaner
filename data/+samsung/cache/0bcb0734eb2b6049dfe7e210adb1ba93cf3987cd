var lastScrollY=0;
function heartBeat(){ 
diffY=document.documentElement.scrollTop; 
percent=0.1*(diffY-lastScrollY); 
if(percent>0)percent=Math.ceil(percent); 
else percent=Math.floor(percent); 
document.getElementById("LeftAD_b2b").style.top=parseInt(document.getElementById("LeftAD_b2b").style.top)+percent+"px";
