<script type="text/javascript">
function s_control_click_SNS(linkname)
{
	var pageType=$("#pageTrack").val();
	if(pageType=="support detail" || pageType=="product detail")
	{
		var modeName=$("#model_name").val();
		s_control_click('products,eVar17,events', 'event15', modeName+','+linkname+',event15', 'o', 'share this product');
	}
	else
	{
		s_control_click('eVar17,events', 'event15', linkname+',event15', 'o', 'share this product');
	}
}
</script>
