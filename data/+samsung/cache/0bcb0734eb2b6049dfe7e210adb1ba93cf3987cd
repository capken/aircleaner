<script type="text/javascript">
//<![CDATA[
$(document).ready(function(){

	//spec pdf omniture
	if($(".reverse").length) {
		$(".reverse").each(function() {
			$(this).find("a").first().click(function() {
				f_omniture();
			});
		});
	}
	
	if($("#dataBindingPageParam > .longGsValUseFl").text() == "Y" &&
			$("#dataBindingPageParam > .newSpecYN").text() == 'N') {
		$("#div_bullet_view_tab > .tableOn > a").attr("href", "javascript:displayTableView()");
		$("#div_table_view_tab > .bulletOn > a").attr("href", "javascript:displayBulletView()");
		
		displayBulletView();
	} else {
		//bullet view 탭을 보여주지않는다.
		$("#div_table_view_tab > .bulletOn").hide();
		
		displayTableView();
	}
	
	$("#content").show();
	

});

function displayBulletView() {
	$("#div_bullet_view_tab"    ).show();
	$("#div_table_view_tab"     ).hide();
	$("#div_bullet_view_content").show();
	$("#div_table_view_content" ).hide();
}

function displayTableView() {
	$("#div_bullet_view_tab"    ).hide();
	$("#div_table_view_tab"     ).show();
	$("#div_bullet_view_content").hide();
	$("#div_table_view_content" ).show();
}

function f_omniture() {
	s_control_click('products,events,eVar9', 'event3', 'SA-T501BA,event3,spec pdf', 'o', 'product interaction');
}

function appPop(mdlCd){
	window.open("/sec/consumer/mobile/Apps/"+mdlCd+".html", "pop", 'width=640, height=700, left=0, top=0, scrollbars=yes');
}

//]]>
</script>


    
    
